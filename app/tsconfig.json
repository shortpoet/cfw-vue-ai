  //   "#/*": [
  //     "./app/*"
  //   ],
  //   "assets": [
  //     "assets"
  //   ],
  //   "assets/*": [
  //     "assets/*"
  //   ],
  //   "public": [
  //     "public"
  //   ],
  //   "public/*": [
  //     "public/*"
  //   ],
  // },
  "include": [
    "api/**/*",
    "types/**/*",
    "bindings.d.ts",
    "utils/request.ts"
  ]
}