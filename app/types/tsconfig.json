{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "@cloudflare/workers-types",
    ],
    "paths": {
      "##": [
        "./"
      ],
      "##/*": [
        "./*"
      ],
      "#": [
        "./"
      ],
      "#/*": [
        "./*"
      ],
      "assets": [
        "assets"
      ],
      "assets/*": [
        "assets/*"
      ],
      "public": [
        "public"
      ],
      "public/*": [
        "public/*"
      ],
    },
  },
  "include": [
    "**/*.ts",
    "../bindings.d.ts",
  ]
}