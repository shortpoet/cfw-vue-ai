{
  "name": "cfw-vue-ai",
  "version": "0.0.2",
  "workspaces": [
    "api",
    "scripts",
    "types",
    "utils",
    "ui"
  ],
  "type": "module",
  "scripts": {
    "========= Util": "",
    "wrangle": "tsx scripts/wrangle.ts",
    "wrangle:kv": "tsx scripts/wrangle.ts --cmds kv",
    "start": "cross-env NODE_ENV=development && concurrently \"pnpm serve\" \"pnpm v:dev\"",
    "dev": "pnpm run v:build && pnpm run w:build && pnpm run w:dev",
    "prev": "pnpm run wrangle:kv && pnpm run w:dev",
    "lint": "eslint --ext .ts,.tsx .",
    "prepare": "cd .. && husky install app/.husky",
    "========= Wskp": "",
    "api": "pnpm --filter api",
    "db": "pnpm --filter api",
    "types": "pnpm --filter types",
    "scripts": "pnpm --filter scripts",
    "serv": "pnpm --filter server",
    "ui": "pnpm --filter ui",
    "utils": "pnpm --filter utils",
    "========= Server": "",
    "serve": "pnpm serv serve",
    "========= Api": "",
    "w:build": "pnpm api build",
    "w:dev": "pnpm api dev",
    "w:deploy": "pnpm api deploy",
    "========= Ui": "",
    "v:build": "pnpm ui build",
    "v:dev": "pnpm ui dev"
  },
  "devDependencies": {
    "@auth/core": "^0.18.4",
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.0",
    "types": "link:@auth/core/types",
    "typescript": "^5.3.2"
  }
}