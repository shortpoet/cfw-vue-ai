{
  "name": "cfw-vue-ai",
  "version": "0.0.1",
  "workspaces": [
    "api",
    "ui",
    "worker"
  ],
  "type": "module",
  "scripts": {
    "========= Util": "",
    "wrangle": "tsx scripts/wrangle.ts",
    "wrangle:kv": "tsx scripts/wrangle.ts --cmds kv",
    "dev": "pnpm run w:dev",
    "prev": "pnpm run wrangle:kv && pnpm run w:dev",
    "lint": "eslint --ext .ts,.tsx .",
    "========= Wskp": "",
    "api": "pnpm --filter api",
    "========= Api": "",
    "w:build": "node scripts/build-worker.mjs --entry ./api/src/index.ts --out ./api/build/worker.mjs",
    "w:dev": "pnpm api dev",
    "w:deploy": "pnpm api deploy",
    "prepare": "cd .. && husky install app/.husky"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20231121.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chalk": "^5.3.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.0",
    "typescript": "^5.3.2"
  }
}