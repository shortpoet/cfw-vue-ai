    "itty-router": "^4.0.23",
    "typescript": "^5.3.2",
    "wrangler": "^3.19.0"
  },
  "dependencies": {
    "@auth/core": "0.18.0",
    "@cfw-vue-ai/db": "workspace:^",
    "@cloudflare/itty-router-openapi": "^1.0.5",
    "kysely": "^0.26.3",
    "openai": "^4.20.1",
